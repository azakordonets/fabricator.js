{
  "name": "fabricator",
  "version": "0.1.0",
  "description": "Generate fake test data",
  "main": "lib/fabricator.js",
  "scripts": {
    "build": "webpack --mode=build",
    "dev": "webpack --progress --colors --watch --mode=dev",
    "lint": "node_modules/eslint/bin/eslint.js . --fix",
    "test": "ava \"test/*.test.js\" --verbose",
    "coverage": "nyc npm t",
    "watch:test": "npm t -- --watch",
    "watch:lint": "node node_modules/eslint-watch/bin/esw -w --fix"
  },
  "devDependencies": {
    "ava": "^0.15.2",
    "babel": "^6.5.2",
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
    "eslint": "^2.11.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.3.0",
    "eslint-plugin-react": "^5.1.1",
    "eslint-watch": "^2.1.11",
    "estraverse-fb": "^1.3.1",
    "nyc": "^6.6.1",
    "sinon": "^1.17.4",
    "webpack": "^1.13.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/azakordonets/fabricator.js"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text",
      "html"
    ]
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "keywords": [
    "webpack",
    "es6",
    "starter",
    "library",
    "test data",
    "fake data",
    "generating",
    "universal",
    "umd"
  ],
  "author": "Andrew Zakordonets",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/azakordonets/fabricator.js/issues"
  },
  "homepage": "https://github.com/azakordonets/fabricator.js",
  "dependencies": {
    "babel-eslint": "^6.0.5",
    "lodash": "^4.13.1",
    "read-file-relative": "^1.2.0",
    "rjson-search": "0.0.6"
  }
}
